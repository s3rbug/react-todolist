{
	"devDependencies": {
		"concurrently": "^7.6.0",
		"husky": "^8.0.0",
		"lint-staged": "^13.1.2",
		"prettier": "^2.8.4"
	},
	"scripts": {
		"start": "concurrently \"yarn start-client\" \"yarn start-server\"",
		"start-client": "cd ./frontend && yarn start",
		"start-server": "cd ./backend && yarn start:dev",
		"prepare": "husky install"
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	},
	"private": "true",
	"dependencies": {}
}
